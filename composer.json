{
    "name": "endroid/soccer-data",
    "description": "Endroid Soccer Data",
    "keywords": ["endroid", "soccer", "data"],
    "homepage": "https://github.com/endroid/SoccerData",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeroen van den Enden",
            "email": "info@endroid.nl",
            "homepage": "http://endroid.nl/"
        }
    ],
    "require": {
        "php": ">=7.1",
        "fabpot/goutte": "^3.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Endroid\\SoccerData\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Endroid\\SoccerData\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
