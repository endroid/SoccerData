{
    "name": "endroid/soccer-data",
    "description": "Endroid Soccer Data",
    "keywords": ["endroid", "soccer", "data", "php"],
    "homepage": "https://github.com/endroid/soccer-data",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeroen van den Enden",
            "email": "info@endroid.nl"
        }
    ],
    "require": {
        "php": ">=7.2",
        "endroid/installer": "^1.0.3",
        "symfony/browser-kit": "^4.3||^5.0",
        "symfony/css-selector": "^3.4||^4.1||^5.0",
        "symfony/http-client": "^4.1||^5.0"
    },
    "require-dev": {
        "endroid/test": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Endroid\\SoccerData\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Endroid\\SoccerData\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
